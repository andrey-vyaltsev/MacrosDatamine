!START CHTREND

!LET $hol_in# = holes
!LET $wrk_field# = AU
!LET $cog# = 0.5

!LET $ou_file# = au_stat

==================================================

!EXTRA    &IN($hol_in#),
          &OUT(hol_mac_tmp),
          @APPROX=0.0

 $wrk_field#_ = $wrk_field#
 saveonly(BHID,FROM,TO,LENGTH,X,Y,Z,A0,B0,$wrk_field#,$wrk_field#_)

GO 

!SORTX    &IN(hol_mac_tmp),
          &OUT(hol_mac_wrk),
          *KEY1(BHID),
          *KEY2(FROM),
          @ORDER=1.0,
          @KEYSFRST=1.0,
          @ROWORDER=1.0,
          @KEYTOL=0.00001

