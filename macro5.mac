          &OUT(hol_mac_tmp1),
          &HOLESOUT(hol_mac_wrk_),
          *VALUE($wrk_field#_),
          *OWCODE(RN),
          @CUTOFF=$cog#,
          @MINORE=3,
          @MAXWASTE=3,
          @MINASFR=0.95,
          @DILUTE=0.0,
          @NARWASTE=2.0,
          @ANYORE2=0.0

!DELETE   &IN(hol_mac_tmp1),
          @CONFIRM=0.0

!STATS    &IN(hol_mac_wrk_),
          &OUT(stat_mac_tmp),
          *F1($wrk_field#),
          @KEYSORT=1.0,
          @KEYTOL=0.00001,
          @PCNTILES=1.0,
          @SORTOUT=1.0,
          RN=1

!DELETE   &IN(hol_mac_wrk_),
          @CONFIRM=0.0

!EXTRA    &IN(stat_mac_tmp),
          &OUT($ou_file#),
          @APPROX=0.0

 COG = $cog#
 MINORE = 3
 MAXWASTE = 3

GO 

!DELETE   &IN(stat_mac_tmp),
          @CONFIRM=0.0

!LET $cog# = $cog# + 0.5

